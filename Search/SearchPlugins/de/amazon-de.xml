<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Amazon.de</ShortName>
<InputEncoding>ISO-8859-1</InputEncoding>
<Image width="16" height="16">https://searchfox.org/mozilla-central/source/browser/components/newtab/data/content/tippytop/images/amazon@2x.png</Image>
<Url type="text/html" method="GET" template="https://www.amazon.de/exec/obidos/external-search/">
  <Param name="field-keywords" value="{searchTerms}"/>
  <Param name="mode" value="blended"/>
  <Param name="tag" value="mozilla-20"/>
  <Param name="sourceid" value="Mozilla-search"/>
</Url>
<SearchForm>https://www.amazon.de/</SearchForm>
</SearchPlugin>
